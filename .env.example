# FreeCAD Manufacturing Co-Pilot Environment Configuration
# Copy this file to .env and fill in your actual API keys

# Anthropic API Key for cloud backend functionality
# Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Text-to-CAD Server Configuration
TEXT_TO_CAD_API_KEY=test-api-key
TEXT_TO_CAD_URL=http://localhost:8084

# Cloud Backend Configuration
CLOUD_BACKEND_URL=http://localhost:8091

# Development Settings
DEBUG=false
LOG_LEVEL=INFO
